{
    "name": "cdk-day",
    "version": "0.1.0",
    "license": "MIT-0",
    "bin": {
        "cdk-day": "bin/cdk-day.js"
    },
    "scripts": {
        "cdk:help": "cdk --help",
        "init": "cdk bootstrap",
        "build": "sam-beta-cdk build",
        "deploy": "cdk deploy --app .aws-sam/build --outputs-file cdk-outputs.json",
        "postdeploy": "ts-node tools/generate-locals && prettier --write locals.json",
        "start": "sam-beta-cdk local start-api -n locals.json --warm-containers EAGER",
        "start:lambda": "sam-beta-cdk local start-lambda -n locals.json"
    },
    "devDependencies": {
        "@aws-cdk/assert": "1.100.0",
        "@types/jest": "^26.0.10",
        "@types/jsonfile": "^6.0.1",
        "@types/node": "^10.17.60",
        "aws-cdk": "1.100.0",
        "jest": "^26.4.2",
        "jsonfile": "^6.1.0",
        "prettier": "^2.5.1",
        "ts-jest": "^26.2.0",
        "ts-node": "^9.0.0",
        "typescript": "~3.9.7"
    },
    "dependencies": {
        "@aws-cdk/aws-apigatewayv2": "^1.100.0",
        "@aws-cdk/aws-apigatewayv2-integrations": "^1.100.0",
        "@aws-cdk/aws-dynamodb": "^1.100.0",
        "@aws-cdk/aws-events": "^1.100.0",
        "@aws-cdk/aws-events-targets": "^1.100.0",
        "@aws-cdk/aws-iam": "^1.100.0",
        "@aws-cdk/aws-lambda": "^1.100.0",
        "@aws-cdk/aws-lambda-nodejs": "^1.100.0",
        "@aws-cdk/core": "1.100.0",
        "esbuild": "^0.11.13",
        "source-map-support": "^0.5.16"
    }
}
