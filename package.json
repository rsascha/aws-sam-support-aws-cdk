{
  "name": "cdk-day",
  "version": "0.1.0",
  "license": "MIT-0",
  "bin": {
    "cdk-day": "bin/cdk-day.js"
  },
  "scripts": {
    "cdk:help": "cdk --help",
    "init": "cdk bootstrap",
    "build": "sam-beta-cdk build",
    "postbuild": "echo \"Better just 'run npm deploy', because locals.json will be created automatically.\"",
    "deploy": "cdk deploy --app .aws-sam/build --outputs-file cdk-outputs.json",
    "postdeploy": "ts-node tools/generate-locals && prettier --write locals.json",
    "start": "sam-beta-cdk local start-api -n locals.json --warm-containers EAGER",
    "start:lambda": "sam-beta-cdk local start-lambda -n locals.json",
    "test:e2e": "npm run test-e2e-1",
    "test-e2e-1": "sam-beta-cdk local invoke CdkDayStack/PutTranslationFunction -e events/putTranslation.json -n locals.json"
  },
  "devDependencies": {
    "@aws-cdk/assert": "2.3.0",
    "@types/jest": "^27.0.3",
    "@types/jsonfile": "^6.0.1",
    "@types/node": "^17.0.5",
    "aws-cdk": "2.3.0",
    "jest": "^27.4.5",
    "jsonfile": "^6.1.0",
    "prettier": "^2.5.1",
    "ts-jest": "^27.1.2",
    "ts-node": "^10.4.0",
    "typescript": "~4.5.4"
  },
  "dependencies": {
    "@aws-cdk/aws-apigatewayv2-integrations": "^1.137.0",
    "@aws-cdk/aws-apigatewayv2": "^1.137.0",
    "@aws-cdk/aws-dynamodb": "^1.137.0",
    "@aws-cdk/aws-events-targets": "^1.137.0",
    "@aws-cdk/aws-events": "^1.137.0",
    "@aws-cdk/aws-iam": "^1.137.0",
    "@aws-cdk/aws-lambda-nodejs": "^1.137.0",
    "@aws-cdk/aws-lambda": "^1.137.0",
    "@aws-cdk/core": "1.137.0",
    "esbuild": "^0.14.8",
    "source-map-support": "^0.5.21"
  }
}
